                     "/min_points - Информацию о минимальных баллах, подтверждающих успешное прохождение вступительных испытаний." + "\n" + #- #естьвтрелло
                     "/reg - Регистрация в личном кабинете абитуриента." + "\n" +#естьвтрелло
                     "/cabinet - Личный кабинет абитуриента." + "\n" +#естьвтрелло
                     "/student_life - Информация о студенческой жизни." + "\n" + #?#естьвтрелло
                     "/contacts - Контактная информация." + "\n" + #? #естьвтрелло
                     "/location - Информация о расположении приемной комиссии университета." + "\n" #+ #естьвтрелло
                     "/special_projects - Информация про специальные проекты."+ "\n" +#естьвтрелло
                     "/partners - Информация о партнерах института." + "\n" +#естьвтрелло
                     "/laboratories - Информация о транспортных лабораториях." #естьвтрелло
                     "/video - Видео с объяснением процедуры зачисления в университет" #естьвтрелло
                     )

@bot.message_handler(commands=['info'])
def tests_info_command(message):
    markup = types.InlineKeyboardMarkup()
    tests_info_button = types.InlineKeyboardButton(text="ИУЦТ", url="https://imiit.ru/")
    markup.add(tests_info_button)
    bot.send_message(message.chat.id,
                     "Институт управления и цифровых технологий это:" + "\n" +
                     "● Более 4200 студентов" + "\n" +
                     "● Более 150 профессоров и кандидатов наук" + "\n" +
                     "● 8 различных кафедр" + "\n" +
                    "(Для того чтобы увидеть информацию о кафедрах используйте - /departments)" + "\n" +
