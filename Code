import telebot
from telebot import types

bot = telebot.TeleBot("")

@bot.message_handler(commands=['start'])
def start_command(message):
    bot.send_message(message.chat.id, "Привет, напиши /info для просмотра всей информации об ИУЦТ, /help - для просмотра всего списка команд")

@bot.message_handler(commands=['help'])
def help_command(message):
    bot.send_message(message.chat.id,
                     "Список возможных команд:" + "\n" +
                     "/overview - Презентационное видео о Российском Университете Транспорта (МИИТ)." + "\n" + #неупоминается #естьвтрелло
                     "/info - Информация о ИУЦТ." + "\n" + #естьвтрелло
                     "/vk - Официальное сообщество ИУЦТ РУТ (МИИТ) в ВКонтакте." + "\n" + #естьвтрелло
                     "/youtube - Канал ИУЦТ на YouTube." + "\n" + #естьвтрелло
                     "/instagram - Инстаграм ИУЦТ." + "\n" + #естьвтрелло
                     "/telegram - Телеграм-канал ИУЦТ." + "\n" + #естьвтрелло
                     "/departments - Информация о кафедрах ИУЦТ." + "\n" + #естьвтрелло
                     "/institute_history - История ИУЦТ." + "\n" +#естьвтрелло
                     "/news - Новости и события ИУЦТ."+ "\n" + #естьвтрелло
                     "/first_persons - Первые лица института."+ "\n" + #естьвтрелло
                     "/advantages - Информация о преимуществах учебы в ИУЦТ."
                     "/programs - Информация о программах обучения в институте." + "\n" + #неупоминается #естьвтрелло
                     '/open_day - Видео: "День открытых дверей РУТ (МИИТ)". Содержит информацию об институтах, включая ИУЦТ.' + "\n" + #неупоминается #естьвтрелло
                     "/entrance22 - Видео об организация приема в 2022 году." + "\n" + #+ #естьвтрелло
                     "/entrance_tests - Информация о вступительных испытаниях, проводимых университетом самостоятельно." + "\n" + #+ #естьвтрелло
