@bot.message_handler(commands=['vk'])
def vk_command(message):
    markup = types.InlineKeyboardMarkup()
    vk_button = types.InlineKeyboardButton(text="ВКонтакте", url="https://vk.com/imdtrut")
    markup.add(vk_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы перейти в официальное сообщество Института управления и цифровых технологий РУТ (МИИТ) в ВКонтакте.", reply_markup=markup)

