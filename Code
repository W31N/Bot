def cabinet_command(message):
    markup = types.InlineKeyboardMarkup()
    cabinet_button = types.InlineKeyboardButton(text="Личный кабинет", url="https://rut-miit.ru/cabinet/hello/login.jsp")
    markup.add(cabinet_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы перейти в личный кабинет абитуриента. Если вы еще не зарегистрированы, используйте - /reg.", reply_markup=markup)

@bot.message_handler(commands=['student_life'])
def student_life_command(message):
    markup = types.InlineKeyboardMarkup()
    student_life_button = types.InlineKeyboardButton(text="Студенческая жизнь", url= "https://rut-miit.ru/org/social/student-live")
    markup.add(student_life_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы узнать о студенческой жизни.", reply_markup=markup)

@bot.message_handler(commands=['reg'])
def reg_command(message):
    markup = types.InlineKeyboardMarkup()
    reg_button = types.InlineKeyboardButton(text="Регистрация", url= "https://rut-miit.ru/reg/faces/welcome")
    markup.add(reg_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы зарегистрироваться.", reply_markup=markup)

@bot.message_handler(commands=['location'])
def reg_command(message):
    markup = types.InlineKeyboardMarkup()
    location_button = types.InlineKeyboardButton(text="Приемная комиссия", url= "https://yandex.ru/maps/-/CCUy4HWc~A")
    markup.add(location_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы узнать расположение приемной комиссии университета.", reply_markup=markup)

@bot.message_handler(commands=['overview'])
def overview_command(message):
    markup = types.InlineKeyboardMarkup()
    overview_button = types.InlineKeyboardButton(text="РУТ (обзорный ролик)", url= "https://www.youtube.com/watch?v=h0yOSNcQFT0&t=1s")
    markup.add(overview_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы просмотреть презентационное видео о Российском Университете Транспорта (МИИТ).", reply_markup=markup)

@bot.message_handler(commands=['entrance22'])
def overview_command(message):
    markup = types.InlineKeyboardMarkup()
    overview_button = types.InlineKeyboardButton(text="Организация приема 2022", url= "https://www.youtube.com/watch?v=Y9K6BXpXgA4")
    markup.add(overview_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы просмотреть видео об организации приема в 2022 году.", reply_markup=markup)

@bot.message_handler(commands=['open_day'])
def open_day_command(message):
    markup = types.InlineKeyboardMarkup()
    open_day_button = types.InlineKeyboardButton(text="День открытых дверей РУТ (МИИТ)", url= "https://www.youtube.com/watch?v=2vxPs0C56Ys&t=131s")
    markup.add(open_day_button)
    bot.send_message(message.chat.id, "Нажмите на кнопку, для того чтобы просмотреть видео, в котором побробно описывается каждый институт, а также имеется инструкция по поиску и подбору нужного вам профиля, специальности или программы обучения.", reply_markup=markup)

bot.polling(none_stop=True)
